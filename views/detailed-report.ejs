                    <div class=" relative flex justify-center mt-6 text-2xl fontcolorblack">
                        <div><%= NPcount%></div>
                    </div>
                    <div>
                        <p class=" flex justify-center text-center mt-1 xxl:text-xl fontcolorblack">NP</p>
                    </div>
                </div>

                <!-- OPN -->
                <div class=" mainbox h-24 w-32 rounded-lg  bg-ColorBlack  hover:scale-125 transition duration-150 ease-out hover:bg-HoverColorBlack hover:border-black text-white relative">
                    <div class=" relative flex justify-center mt-6 text-2xl fontcolorblack">
                        <div><%= OPNcount%></div>
