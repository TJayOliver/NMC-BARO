<!DOCTYPE html>
<html lang="en" class=" scroll-smooth overflow-x-hidden ">
<head>
    <%- include("layout/head") %>
</head>
<body class=" min-h-screen p-0 m-0 select-none w-full bg-backgroundColorBlack transition duration-500 ease-in">
    <%- include("layout/header-smallscreen") %>

    <!-- Main Heading -->
    <h1 class=" flex justify-center text-center mt-12 text-4xl text-white fontcolorblack">CLIENT DETAILS</h1>

    <div class="mt-5 p-3 overflow-auto max-w-5xl xl:max-w-7xl transition duration-100 ease-linear">
        <% for(data of data){ %> 
        <div class=" text-white p-4"><%= data.registration_number %></div>
        <div class=" text-white p-4"><%= data.first_name %></div>
        <div class=" text-white p-4"><%= data.middle_name %></div>
        <div class=" text-white p-4"><%= data.last_name %></div>
        <div class=" text-white p-4"><%= data.date_of_birth %></div>
        <div class=" text-white p-4"><%= data.gender %></div>
        <div class=" text-white p-4"><%= data.programme %></div>
        <div class=" text-white p-4"><%= data.email_address %></div>
        <div class=" text-white p-4"><%= data.contact_address %></div>
        <div class=" text-white p-4"><%= data.phone_number %></div>
        <div class=" text-white p-4"><%= data.index_number %></div>
        <% } %> 
    </div>
  
    <script src="scripts/allPages.js"></script>
</body>
</html>



